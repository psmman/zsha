# Alias mocp
#mocp info
alias mi="mocp -i"
#mocp pause/unpause
alias mp="mocp -G"
#mocp stop
alias ms="mocp -s"
#mocp exit
alias mx="mocp -x"
#mocp next
alias mf="mocp -f"
#mocp prev
alias mr="mocp -r"
#mocp volumen +5
alias m.="mocp --volume=+5 LEVEL"
#mocp volumen -5
alias m,="mocp --volume=-5 LEVEL"
#mocp volumen +10
alias m..="mocp --volume=+10 LEVEL"
#mocp volumen -10
alias m,,="mocp --volume=-10 LEVEL"

